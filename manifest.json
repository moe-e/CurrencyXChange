{
  "manifest_version": 2,
  "name": "CurrencyXChange",
  "description": "Quick and simple currency converter for Google Chrome.",
  "version": "1.0",
  "icons": {
    "16": "images/currency.png",
    "48": "images/currency.png",
    "128": "images/currency.png"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon":"images/currency.png"
  },
  "permissions": ["activeTab"]
}